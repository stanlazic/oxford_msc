@Article{Bello2014,
  Title                    = {Lack of blinding of outcome assessors in animal model experiments implies risk of observer bias.},
  Author                   = {Segun Bello and Lasse T Krogsboll and Jan Gruber and Zhizhuang J Zhao and Doris Fischer and Asbjorn Hrobjartsson},
  Journal                  = {J Clin Epidemiol},
  Year                     = {2014},

  Month = {},
  Number                   = {9},
  Pages                    = {973--983},
  Volume                   = {67},

  Abstract                 = {To examine the impact of not blinding outcome assessors on estimates of intervention effects in animal experiments modeling human clinical conditions.We searched PubMed, Biosis, Google Scholar, and HighWire Press and included animal model experiments with both blinded and nonblinded outcome assessors. For each experiment, we calculated the ratio of odds ratios (ROR), that is, the odds ratio (OR) from nonblinded assessments relative to the corresponding OR from blinded assessments. We standardized the ORs according to the experimental hypothesis, such that an ROR <1 indicates that nonblinded assessor exaggerated intervention effect, that is, exaggerated benefit in experiments investigating possible benefit or exaggerated harm in experiments investigating possible harm. We pooled RORs with inverse variance random-effects meta-analysis.We included 10 (2,450 animals) experiments in the main meta-analysis. Outcomes were subjective in most experiments. The pooled ROR was 0.41 (95\% confidence interval [CI], 0.20, 0.82; I(2) = 75\%; P < 0.001), indicating an average exaggeration of the nonblinded ORs by 59\%. The heterogeneity was quantitative and caused by three pesticides experiments with very large observer bias, pooled ROR was 0.20 (95\% CI, 0.07, 0.59) in contrast to the pooled ROR in the other seven experiments, 0.82 (95\% CI, 0.57, 1.17).Lack of blinding of outcome assessors in animal model experiments with subjective outcomes implies a considerable risk of observer bias.},
  Doi                      = {10.1016/j.jclinepi.2014.04.008},
  Institution              = {The Nordic Cochrane Centre, Rigshospitalet, Department 7811, Blegdamsvej 9, 2100 Copenhagen Ø, Denmark.},
