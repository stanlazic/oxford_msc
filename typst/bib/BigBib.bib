  groups    = {Me},
  keywords  = {Algorithms; Animals; Autoradiography; Bias (Epidemiology); Brain; Carbon Radioisotopes; Gene Expression; Gene Expression Profiling; Image Processing, Computer-Assisted; In Situ Hybridization; Male; Models, Statistical; Rats; Rats, Sprague-Dawley},
