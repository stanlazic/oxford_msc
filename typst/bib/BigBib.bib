@Article{Kalueff2006,
  Title                    = {Hair barbering in mice: implications for neurobehavioural research.},
  Author                   = {A. V. Kalueff and A. Minasyan and T. Keisala and Z. H. Shah and P. Tuohimaa},
  Journal                  = {Behav Processes},
