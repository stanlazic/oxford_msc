@Article{Leek2010,
  Title                    = {Tackling the widespread and critical impact of batch effects in high-throughput data.},
  Author                   = {Jeffrey T Leek and Robert B Scharpf and Hector Corrada Bravo and David Simcha and Benjamin Langmead and W. Evan Johnson and Donald Geman and Keith Baggerly and Rafael A Irizarry},
  Journal                  = {Nat Rev Genet},
