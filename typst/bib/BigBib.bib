@Article{Chen2007,
  author      = {Henian Chen and Patricia Cohen and Sophie Chen},
  title       = {Biased odds ratios from dichotomization of age.},
  journal     = {Stat Med},
  year        = {2007},
  volume      = {26},
  number      = {18},
  pages       = {3487--3497},
  month = {},
  abstract    = {Dichotomizing a continuous variable is known to result in the loss of information, lower statistical power, and lower reliability. In many epidemiological studies, age is a scaled (continuous) variable prior to statistical analyses; however, despite pleas from methodologists, researchers frequently dichotomize age in their data analysis without an appropriate rationale. Using simulated case-control data, we show that dichotomizing age generally will lead to a biased odds ratio (OR). When age was a confounder (potentially representing common causes of risks and outcomes), including age as a scaled variable (whether the age effect was linear or non-linear in the logit), provided satisfactory control, whereas when age was categorized, the estimated risk factor effect was biased. We also demonstrate that the further the cutpoint is from the median age, the greater the increase in the OR; thus, in cases where age dichotomization is warranted, researchers are cautioned not to allow the size of the empirical OR to influence their choice of cutpoint. Recommendations are made for analysing age in epidemiological data and interpretation of empirical findings.},
  doi         = {10.1002/sim.2737},
  groups      = {Statistics},
  institution = {Epidemiology of Mental Disorders, New York State Psychiatric Institute, New York, USA. chenhen@pi.cpmc.columbia.edu},
  keywords    = {Adult; Age Factors; Bias (Epidemiology); Confounding Factors (Epidemiology); Data Interpretation, Statistical; Epidemiologic Studies; Humans; Middle Aged; Odds Ratio; Reproducibility of Results},
  language    = {eng},
  medline-pst = {ppublish},
  owner       = {sel},
  pmid        = {17066378},
  timestamp   = {2015.04.19},
  url = {},
}

@Article{Chevallier2005,
  author    = {Nathalie L Chevallier and Salvador Soriano and David E Kang and Eliezer Masliah and Gloria Hu and Edward H Koo},
  title     = {{Perturbed neurogenesis in the adult hippocampus associated with presenilin-1 A246E mutation.}},
  journal   = {Am J Pathol},
  year      = {2005},
  volume    = {167},
  number    = {1},
  pages     = {151--159},
  abstract  = {In addition to its well-established role in gamma-secretase cleavage, presenilin (PS) also plays a role in regulating the stability of cytosolic beta-catenin, a protein involved in Wnt signaling. Several familial Alzheimer's disease-associated PS1 mutations have been shown to increase the stability of the signaling pool of beta-catenin, correlating with enhanced cell proliferation. Accordingly, we hypothesized that in the setting of PS1 mutations, abnormal activation of Wnt/beta-catenin signaling leads to increased cell division. We tested this hypothesis by examining whether there is evidence of increased neurogenesis in the hippocampus of adult transgenic mice that overexpress the PS1 A246E mutation. In PS1/PS2-deficient fibroblasts, expression of PS1 A246E Familial AD mutation failed to restore the rapid turnover of beta-catenin compared with wild-type PS1. We then examined whether the same mutation enhanced neurogenesis in vivo in adult hippocampus of PS1-deficient mice when restored by wild-type human PS1 (PS1(-/-)WT) or A246E PS1 mutation (PS1(-/-)AE). The PS1 A246E mutation stimulated the proliferation of progenitor cells in the dentate gyrus of adult mice, as assessed by 5-bromo-2-deoxyuridine incorporation, but did not influence their survival or differentiation. These observations suggest that the PS1 A246E mutation influences cell growth putatively via abnormal beta-catenin signaling in vivo.},
  groups    = {Alzheimer's, Neurogenesis},
  keywords  = {. Gov't, Alzheimer Disease, Animals, Cell Death, Cell Differentiation, Cell Proliferation, Cell Survival, Cytoskeletal Proteins, Extramural, Fluorescent Antibody Technique, Hippocampus, Humans, Immunoblotting, Membrane Proteins, Mice, Mutation, N.I.H., Neurons, Non-U.S. Gov't, P.H.S., Research Support, Stem Cells, Trans-Activators, Transgenic, U.S, beta Catenin, 15972961},
  pii       = {167/1/151},
  pmid      = {15972961},
  timestamp = {2006.06.30},
}

