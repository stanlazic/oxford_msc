  timestamp = {2006.10.14},
}

@Article{Hubbard2003,
  Title                    = {Confusion over measures of evidence (p's) versus errors ($\alpha$'s) in classical statistical testing.},
  Author                   = {R Hubbard and M J Bayarri},
  Journal                  = {American Statistician},
  Year                     = {2003},
