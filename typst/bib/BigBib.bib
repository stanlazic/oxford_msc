}

@Article{Couillard-Despres2005,
  author    = {Sebastien Couillard-Despres and Beate Winner and Susanne Schaubeck and Robert Aigner and Maurice Vroemen and Norbert Weidner and Ulrich Bogdahn and J�rgen Winkler and Hans-Georg Kuhn and Ludwig Aigner},
